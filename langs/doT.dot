<!-- doT is a templating engine for generating dynamic HTML content. -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Hello, World!</title>
</head>
<body>
    <h1>{{=it.title}}</h1>
    <ul>
        {{ for (var i = 0; i < it.items.length; i++) { }}
            <li>{{=it.items[i]}}</li>
        {{ } }}
    </ul>
</body>
</html>